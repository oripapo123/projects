<Window x:Class="GUI.RetreivePassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RetreivePassword" Height="95" Width="337"
        ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="0,4,0,7" TextWrapping="Wrap" Text="Enter Email:" Width="67"/>
        <TextBox Name="Email" HorizontalAlignment="Left" Height="21" Margin="72,1,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="159"/>
        <Button Content="OKAY" HorizontalAlignment="Left" Height="21" Margin="236,0,0,0" VerticalAlignment="Top" Width="83" Click="Button_Click"/>
        <Button Content="Back" HorizontalAlignment="Left" Height="27" Margin="10,30,0,0" VerticalAlignment="Top" Width="41" Click="Button_Click_1"/>
    </Grid>
</Window>
