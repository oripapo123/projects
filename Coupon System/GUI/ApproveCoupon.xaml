<Window x:Class="GUI.ApproveCoupon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ApproveCoupon" Height="350" Width="525"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize">
    <Grid>

        <DataGrid x:Name="CouponGrid" ItemsSource="{Binding}" HorizontalAlignment="Left" Height="191" Margin="0,89,0,0" VerticalAlignment="Top" Width="509" SelectionChanged="CouponGrid_SelectionChanged_1"/>
        <Button Name="ShowCoupons" Content="Show coupons" HorizontalAlignment="Left" Height="36" Margin="322,31,-187,0" VerticalAlignment="Top" Width="157" Click="Button_Click"/>
        <Button Name="Approve" Content="Approve!" HorizontalAlignment="Left" Height="36" Margin="29,31,0,0" VerticalAlignment="Top" Width="136" Click="Approve_Click"/>
        <Button Content="Back" HorizontalAlignment="Left" Height="27" Margin="10,285,0,0" VerticalAlignment="Top" Width="41" Click="Button_Click_1"/>
    </Grid>

</Window>
