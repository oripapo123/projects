        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="BusinessCoupons" Height="350" Width="525"
        ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="Back" HorizontalAlignment="Left" Height="27" Margin="10,285,0,0" VerticalAlignment="Top" Width="41" Click="Button_Click_1"/>

        <DataGrid Name="BusinessGrid" ItemsSource="{Binding}" HorizontalAlignment="Left" Height="191" Margin="0,89,0,0" VerticalAlignment="Top" Width="509"/>
        <Button Content="Show Business coupons" HorizontalAlignment="Left" Height="36" Margin="322,31,0,0" VerticalAlignment="Top" Width="157" Click="Button_Click"/>
    </Grid>
</Window>
